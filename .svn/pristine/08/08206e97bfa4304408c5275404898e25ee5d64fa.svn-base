package lunacorpustools.POSTagger;

public class WordLengthData {


	int length = 0;
	String pos = null;
	double p = 0.0;
		
	public WordLengthData(int length, String pos, double p) {
		super();
		this.length = length;
		this.pos = pos;
		this.p = p;
	}

	public String getAll(){
		return length + " " + pos + " " + p;
		
	}
	public int getLength() {
		return length;
	}

	public void setLength(int length) {
		this.length = length;
	}

	public String getPos() {
		return pos;
	}

	public void setPos(String pos) {
		this.pos = pos;
	}

	public double getP() {
		return p;
	}

	public void setP(double p) {
		this.p = p;
	}

