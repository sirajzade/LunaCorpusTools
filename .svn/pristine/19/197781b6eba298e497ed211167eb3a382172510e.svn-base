package lunacorpustools;

import java.util.ArrayList;
import javax.swing.JCheckBox;
import javax.swing.JPanel;

/**
 *
 * @author joshgun.sirajzade
 */
public class JPanelForTypes  extends JPanel{
    ArrayList elements;
    JCheckBox ja;
    JCheckBox nein;

    public void setJa(JCheckBox ja) {
        this.ja = ja;
    }

    public void setNein(JCheckBox nein) {
        this.nein = nein;
    }

    public JCheckBox getJa() {
        return ja;
    }

    public JCheckBox getNein() {
        return nein;
    }
    
    public JPanelForTypes(){
        super();
    }
    
    public void setElements(ArrayList els){
        elements = els;
    }
            
    public ArrayList getElements(){
        return elements;
    }
    
}
    
    
    

